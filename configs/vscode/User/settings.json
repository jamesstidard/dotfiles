{
    "files.autoSave": "afterDelay",
    "editor.fontSize": 14,
    "editor.fontFamily": "SF Mono",
    "editor.multiCursorModifier": "ctrlCmd",
    "files.exclude": {
        "**/__pycache__": true,
        "**/.idea": true,
        "**/.mypy_cache": true,
        "**/.pytest_cache": true,
        "**/*.egg-info": true
    },
    "files.associations": {
        "Corefile": "caddyfile"
    },
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "vue",
    ],
    "editor.copyWithSyntaxHighlighting": false,
    "breadcrumbs.enabled": false,
    "python.jediEnabled": false,
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "[markdown]": {
        "files.trimTrailingWhitespace": false
    },
    "[javascript]": {
        "editor.tabSize": 2,
    },
    "[vue]": {
        "editor.tabSize": 2,
    },
    "[html]": {
        "editor.tabSize": 2,
        "editor.defaultFormatter": "vscode.html-language-features",
    },
    "[css]": {
        "editor.tabSize": 2,
    },
    "[stylus]": {
        "editor.tabSize": 2,
    },
    "workbench.commandPalette.preserveInput": true,
    "explorer.confirmDelete": false,
    "workbench.settings.openDefaultSettings": true,
    "workbench.settings.useSplitJSON": true,
    "window.restoreWindows": "all",
    "window.title": "${rootName}${separator}${activeEditorShort}",
    "explorer.confirmDragAndDrop": false,
    "search.quickOpen.includeSymbols": true,
    "search.smartCase": true,
    "debug.inlineValues": true,
    "terminal.external.osxExec": "iTerm.app",
    "terminal.integrated.fontFamily": "SF Mono",
    "git.autoStash": true,
    "git.fetchOnPull": true,
    "git.autofetch": true,
    "git.defaultCloneDirectory": "/Users/james/Development",
    "git.enableSmartCommit": true,
    "git.path": "/usr/local/bin/hub",
    "python.diagnostics.sourceMapsEnabled": false,
    "python.disableInstallationCheck": true,
    "workbench.statusBar.visible": true,
    "workbench.activityBar.visible": true,
    "diffEditor.ignoreTrimWhitespace": true,
    "python.venvPath": "/Users/james/.local/share/virtualenvs",
    "python.linting.mypyEnabled": false,
    "python.linting.pylintEnabled": false,
    "python.linting.flake8Enabled": true,
    "python.linting.pycodestyleEnabled": false,
    "editor.rulers": [
        90
    ],
    "python.linting.flake8CategorySeverity.E": "Information",
