    "docker.showStartPage": true,
    "python.linting.flake8Path": "/Users/james/.local/bin/flake8",
    "python.formatting.blackPath": "/Users/james/.local/bin/black",
    "docker.commands.build": "docker build --ssh default --rm -f \"${dockerfile}\" -t ${tag} \"${context}\"",
    "Lua.telemetry.enable": true,
    "redhat.telemetry.enabled": true,
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "cmake.configureOnEdit": false,
    "git.autofetchPeriod": 60,
