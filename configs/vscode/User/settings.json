        "FIG_NEW_SESSION": "1"
    },
    "editor.accessibilitySupport": "off",
    "terminal.integrated.defaultProfile.osx": "fish",
    "explorer.autoReveal": false,
    "python.linting.mypyPath": "/Users/james/.local/bin/mypy",
    "python.linting.mypyArgs": [
        "--follow-imports=silent",
        "--ignore-missing-imports",
        "--show-column-numbers",
        "--no-pretty",
        "--strict"
    ],
    "docker.containers.label": "RepositoryNameAndTag",
    "docker.containers.description": [
        "Status"
    ],
    "docker.containers.groupBy": "Registry",
    "testing.automaticallyOpenPeekView": "never",
    "testing.defaultGutterClickAction": "contextMenu",
    "explorer.fileNesting.expand": false,
    "explorer.fileNesting.patterns": {
        "*.ts": "${capture}.js",
        "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
        "*.jsx": "${capture}.js",
        "*.tsx": "${capture}.ts",
        "tsconfig.json": "tsconfig.*.json",
        "package.json": "package-lock.json, yarn.lock",
        "Dockerfile": "*.Dockerfile, docker-compose.yml, .dockerignore",
        ".env": "*.env",
        "pyproject.toml": "poetry.lock, Pipfile, Pipfile.lock, setup.py",
        ".gitignore": ".gitmodules"
    },
    "explorer.fileNesting.enabled": true,
    "isort.path": "isort",
